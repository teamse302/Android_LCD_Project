<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mertdesktop.schoolproject" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2" >
            <intent-filter>
                <action android:name="com.example.mertdesktop.schoolproject.Main2Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Program"
            android:label="@string/title_activity_program" >
            <intent-filter>
                <action android:name="com.example.mertdesktop.schoolproject.Program" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ListChangePopUp"
            android:theme="@style/AppTheme.CustomTheme"
            ></activity>
        <service android:name=".Service"
            android:exported="false"
            ></service>
        <service  android:name=".NotificationClass"
            android:exported="false"
            android:label="@string/title_activity_notification">
            <intent-filter>
                <action android:name="com.example.mertdesktop.schoolproject.Notification" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            ></service>
        <service  android:name=".NotificationEmptyClass"
            android:exported="false"
            android:label="@string/title_activity_notification">
            <intent-filter>
                <action android:name="com.example.mertdesktop.schoolproject.Notification" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            ></service>
    </application>

</manifest>
